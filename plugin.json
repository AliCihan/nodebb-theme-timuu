{
	"id": "nodebb-theme-quickstart",
	"library": "./lib/theme.js",
	"hooks": [
		{
			"hook": "static:app.load", "method": "init"
		},
		{
			"hook": "filter:admin.header.build", "method": "addAdminNavigation"
		},
		{
			"hook": "filter:config.get", "method": "getConfig"
		},
		{
			"hook": "static:assets.prepare", "method": "modifyLessVar"
		}
	],
	"scripts": [
		"lib/client.js",
		"../nodebb-theme-material/lib/material.js",
		"../nodebb-theme-material/lib/vendor/waves.min.js"
	],
	"acpScripts": [
		"../nodebb-theme-material/lib/admin.js"
	]
}